<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="BackgroundColor">#FFF2F2F2</SolidColorBrush>
    <SolidColorBrush x:Key="SubBackgroundColor">White</SolidColorBrush>

    <Color x:Key="ColorBackgroundColor">#FFF2F2F2</Color>
    <Color x:Key="ColorSubBackgroundColor">White</Color>

    <SolidColorBrush x:Key="ForegroundColor">#FF000000</SolidColorBrush>
    <SolidColorBrush x:Key="ForegroundColor_20">#33000000</SolidColorBrush>
    <SolidColorBrush x:Key="ForegroundColor_40">#66000000</SolidColorBrush>
    <SolidColorBrush x:Key="ForegroundColor_60">#99000000</SolidColorBrush>
    <SolidColorBrush x:Key="ForegroundColor_80">#FF000000</SolidColorBrush>

    <SolidColorBrush x:Key="IconColor">#8a69fa</SolidColorBrush>
    <SolidColorBrush x:Key="ForegroundInIconColor">White</SolidColorBrush>


    <Color x:Key="ColorForegroundColor">#FF000000</Color>
    <Color x:Key="ColorForegroundColor_20">#33000000</Color>
    <Color x:Key="ColorForegroundColor_40">#66000000</Color>
    <Color x:Key="ColorForegroundColor_60">#99000000</Color>
    <Color x:Key="ColorForegroundColor_80">#FF000000</Color>
    <Color x:Key="ColorIconColor">#8a69fa</Color>
</ResourceDictionary>