<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="BackgroundColor">#0f0e0e</SolidColorBrush>
    <SolidColorBrush x:Key="SubBackgroundColor">#333333</SolidColorBrush>

    <Color x:Key="ColorBackgroundColor">#0f0e0e</Color>
    <Color x:Key="ColorSubBackgroundColor">#333333</Color>

    <SolidColorBrush x:Key="ForegroundColor">#F0F0F0</SolidColorBrush>
    <SolidColorBrush x:Key="ForegroundColor_20">#33F0F0F0</SolidColorBrush>
    <SolidColorBrush x:Key="ForegroundColor_40">#66F0F0F0</SolidColorBrush>
    <SolidColorBrush x:Key="ForegroundColor_60">#99F0F0F0</SolidColorBrush>
    <SolidColorBrush x:Key="ForegroundColor_80">#CCF0F0F0</SolidColorBrush>

    <SolidColorBrush x:Key="IconColor">#8a69fa</SolidColorBrush>
    <SolidColorBrush x:Key="ForegroundInIconColor">White</SolidColorBrush>


    <Color x:Key="ColorForegroundColor">#F0F0F0</Color>
    <Color x:Key="ColorForegroundColor_20">#33F0F0F0</Color>
    <Color x:Key="ColorForegroundColor_40">#66F0F0F0</Color>
    <Color x:Key="ColorForegroundColor_60">#99F0F0F0</Color>
    <Color x:Key="ColorForegroundColor_80">#CCF0F0F0</Color>
    <Color x:Key="ColorIconColor">#8a69fa</Color>
</ResourceDictionary>