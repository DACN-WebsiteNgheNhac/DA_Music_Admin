<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:Int32 x:Key="FPS">60</sys:Int32>
    <sys:Double x:Key="TextSize">15</sys:Double>
    <sys:Double x:Key="SmallTextSize">14</sys:Double>
    <sys:Double x:Key="TextSizeError">11</sys:Double>
    <sys:Double x:Key="CornerRadius">7</sys:Double>
    <Thickness x:Key="CornerRadius_V2">7</Thickness>
    <CornerRadius x:Key="CornerRadius_V3">7</CornerRadius>

    <sys:String x:Key="FormatDate">dd-MM-yyyy hh:mm</sys:String>
    <sys:String x:Key="FormatDateWithoutTime">dd-MM-yyyy</sys:String>
</ResourceDictionary>